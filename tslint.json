{
  "extends": ["tslint-react", "./node_modules/gts/tslint.json"],
  "rules": {
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "no-unused-variable": true,
    "quotemark": [true, "single", "jsx-double"],
    "variable-name": [
      true,
      "check-format",
      "ban-keywords",
      "allow-leading-underscore",
      "allow-trailing-underscore",
      "allow-pascal-case"
    ]
  }
}
